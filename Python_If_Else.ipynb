      "source": [
        "N = int(input())\n",
        "\n",
        "if N % 2 != 0:\n",
        "    print(\"Weird\")\n",
        "else:\n",
        "    if N >= 2 and N <= 5:\n",
        "        print(\"Not Weird\")\n",
        "    elif N >= 6 and N <= 20:\n",
        "        print(\"Weird\")\n",
        "    elif N > 20:\n",
        "        print(\"Not Weird\")"
      ],
